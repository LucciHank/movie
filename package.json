{
  "type": "module",
  "scripts": {
    "dev": "npm --prefix client run start",
    "dev:server": "npm --prefix server run start",
    "dev:client": "npm --prefix client run start",
    "dev:all": "npx concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "cd client && npm run build",
    "start": "npm --prefix server run start",
    "setup": "npm --prefix server install && npm --prefix client install",
    "postinstall": "npm run setup"
  },
  "dependencies": {
    "apexcharts": "^4.7.0",
    "mongoose": "^8.16.1",
    "react-apexcharts": "^1.7.0",
    "web-vitals": "^5.0.3"
  }
}